    "format",
    "create",
    "object",
    "url"
  ],
  "author": "@joaquimserafim",
  "license": "ISC",
  "devDependencies": {
    "istanbul": "^0.3.5",
    "jscs": "^1.11.1",
    "jshint": "^2.6.0",
    "pre-commit": "^1.0.2",
    "tape": "^3.5.0"
  },
  "dependencies": {
    "is-js-obj-empty": "^1.0.1",
